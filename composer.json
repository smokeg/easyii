{
    "name": "smokeg/easyii",
    "version": "v0.9",
    "description": "Easy CMS based on Yii2 Framework - Development package",
    "keywords": ["easyii"],
    "homepage": "http://easyiicms.com",
    "type": "yii2-extension",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "support": {
        "wiki": "http://easyiicms.com/docs",
        "source": "https://github.com/noumo/easyii.git",
        "issues": "https://github.com/noumo/easyii/issues"
    },
    "authors": [
        {
            "name": "Ivan Vasilkov",
            "email": "noumohope@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "creocoder/yii2-nested-sets": "0.9.*",
        "bower-asset/jquery.switcher": "*@dev",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "4.17.47",
        "2amigos/yii2-selectize-widget": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "yii\\easyii\\": "/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
